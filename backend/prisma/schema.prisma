// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Scanning {
  id Int @id
  status String
  timescan DateTime @default(now())
  students Student[]
}

model Student {
  id Int @id
  nama String
  kelas String
  panggilan String
  kelamin String
  scan Scanning @relation(fields: [scanid], references: [id])
  scanid Int
}